{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "palette": {
    "dark": "#424242",
    "light": "#e9e9e9",
    "pink": "#FF1E8E",
    "green": "#8EFF1E",
    "orange": "#FF8E1E",
    "blue": "#1E8EFF",
    "purple": "#8E1EFF",
    "turq": "#1EFF8E",
    "red": "#FF000F",
    "mred": "#c03c3c"
  },
  "blocks": [
    {
      "alignment": "left",
      "newline": false,
      "segments": [
        {
          "foreground": "p:red",
          "properties": {
            "display_host": true
          },
          "template": "{{ if .SSHSession }} {{ end }}{{ if .Root }} {{end}}",
          "type": "session",
          "style": "plain"
        },
        {
          "type": "os",
          "style": "plain",
          "foreground": "p:light",
          "template": " {{.Icon}}",
          "properties": {
            "macos": "mac"
          }
        },
        {
          "foreground": "p:turq",
          "properties": {
            "branch_icon": " ",
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true,
            "fetch_worktree_count": true
          },
          "style": "diamond",
          "template": " {{ .UpstreamIcon }}{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }}  {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }}  {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }}  {{ .StashCount }}{{ end }} ",
          "type": "git"
        },
        {
          "type": "dotnet",
          "style": "plain",
          "foreground": "p:blue",
          "template": "  {{ .Full }} "
        },
        {
          "type": "node",
          "style": "plain",
          "foreground": "p:green",
          "template": "  {{ .Full }} "
        },
        {
          "type": "angular",
          "style": "plain",
          "foreground": "p:mred",
          "template": " {{ if .Error }}{{ else }} {{ .Full }}{{ end }} "
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "right",
      "newline": false,
      "segments": [
        {
          "type": "exit",
          "style": "plain",
          "foreground": "p:green",
          "foreground_templates": [
            "{{ if gt .Code 0 }}p:red{{ end }}"
          ],
          "template": "{{ if gt .Code 0 }}󱐰 {{ .Meaning }}{{ else }}{{ end }}",
          "properties": {
            "always_enabled": true
          }
        },
        {
          "type": "executiontime",
          "style": "plain",
          "foreground": "p:purple",
          "template": " 󱎫 {{ .FormattedMs }}",
          "properties": {
            "always_enabled": true,
            "style": "austin"
          }
        },
        {
          "type": "battery",
          "style": "plain",
          "foreground": "p:turq",
          "foreground_templates": [
            "{{if eq \"Charging\" .State.String}}p:green{{end}}",
            "{{if eq \"Discharging\" .State.String}}p:orange{{end}}",
            "{{if eq \"Full\" .State.String}}p:turq{{end}}"
          ],
          "template": "{{ if not .Error }}{{ .Icon }}{{ .Percentage }}{{ end }}%  ",
          "properties": {
            "discharging_icon": "󰂃",
            "charging_icon": "󰂋",
            "charged_icon": "󰁹"
          }
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": false,
      "segments": [
        {
          "type": "az",
          "style": "plain",
          "foreground": "p:blue",
          "template": "ﴃ {{ .Name }}",
          "properties": {
            "display_default": true
          }
        },
        {
          "type": "aws",
          "style": "plain",
          "foreground": "p:orange",
          "template": " \udb83\ude0f {{.Profile}}{{if .Region}}@{{.Region}}{{end}}"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "right",
      "newline": false,
      "segments": [
        {
          "type": "kubectl",
          "style": "plain",
          "foreground": "p:pink",
          "template": " ﴱ {{.Context}}{{if .Namespace}} :: {{.Namespace}}{{end}} "
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "p:purple",
          "properties": {
            "folder_icon": "",
            "home_icon": "\uf015",
            "style": "agnoster_full"
          },
          "style": "plain",
          "template": "  {{ .Path }} ",
          "type": "path"
        }
      ],
      "type": "prompt"
    }
  ],
  "console_title_template": "{{ .Folder }}",
  "transient_prompt": {
    "background": "transparent",
    "foreground": "p:light",
    "template": "\ue795 "
  },
  "version": 2
}